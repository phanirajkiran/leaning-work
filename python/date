from datetime import date

d0 = date(2008, 8, 18)
d1 = date(2008, 9, 26)
delta = d0 - d1
print delta.days

>>> a = datetime.now() + timedelta(hours=12)
>>> a.strftime("%Y-%m-%d")
'2016-05-20'
